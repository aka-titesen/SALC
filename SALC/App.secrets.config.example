<?xml version="1.0" encoding="utf-8"?>
<!-- 
  ARCHIVO DE EJEMPLO - COPIAR COMO App.secrets.config
  
  INSTRUCCIONES:
  1. Copia este archivo: App.secrets.config.example -> App.secrets.config
  2. Edita App.secrets.config con tus credenciales reales
  3. App.secrets.config NO se subira a Git (protegido por .gitignore)
  
  PARA GMAIL:
  1. Activa verificacion en 2 pasos: https://myaccount.google.com/security
  2. Genera contrasena de aplicacion: https://myaccount.google.com/apppasswords
  3. Usa la contrasena de 16 caracteres (ej: "abcd efgh ijkl mnop")
-->
<appSettings>
  <!-- Credenciales SMTP - REEMPLAZAR CON TUS DATOS -->
  <add key="SMTP_Usuario" value="tu-email@gmail.com" />
  <add key="SMTP_Password" value="xxxx xxxx xxxx xxxx" />
</appSettings>
